{
    "name": "okvpn/packeton",
    "description": "Private PHP package repository for vendors",
    "keywords": ["packeton", "composer", "private-packagist", "packagist", "satis"],
    "homepage": "https://github.com/vtsykun/packeton",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Nils Adermann",
            "email": "naderman@naderman.de",
            "homepage": "http://www.naderman.de"
        },
        {
            "name": "Jordi Boggiano",
            "email": "j.boggiano@seld.be",
            "homepage": "http://seld.be"
        },
        {
            "name": "Uladzimir Tsykun",
            "email": "vtsykun@okvpn.org",
            "homepage": "https://github.com/vtsykun"
        }
    ],
    "support": {
        "email": "vtsykun@okvpn.org"
    },
    "autoload": {
        "psr-4": { "Packagist\\": "src/Packagist/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ],
        "exclude-from-classmap": [ "src/Packagist/WebBundle/Tests/" ]
    },
    "require": {
        "php": ">=7.2",
        "cebe/markdown": "^1.1",
        "composer/composer": "^1.9",
        "doctrine/doctrine-bundle": "^1.2",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/orm": "^2.6",
        "ezyang/htmlpurifier": "^4.6",
        "friendsofsymfony/user-bundle": "^2.1",
        "incenteev/composer-parameter-handler": "^2.0",
        "nelmio/cors-bundle": "^1.5",
        "knplabs/knp-menu-bundle": "^2.1",
        "okvpn/cron-bundle": "^0.1.0",
        "oro/doctrine-extensions": "^1.2",
        "pagerfanta/pagerfanta": "^1.0",
        "predis/predis": "^1.0",
        "seld/signal-handler": "^1.1",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "snc/redis-bundle": "^2.0",
        "symfony/http-client": "^5.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/symfony": "^3.4",
        "twig/extensions": "^1.0",
        "white-october/pagerfanta-bundle": "^1.0",
        "zendframework/zend-feed": "^2.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^2.7 || ^3.0",
        "sensio/generator-bundle": "^3.0",
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('app/config/parameters.yml') || copy('app/config/parameters.yml.dist', 'app/config/parameters.yml');\"",
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink"
    }
}
